{
    "name": "Site specific prefetcher",
    "version": "1.0",
    "description": "Insert prefetch tags in specific sites",
    "content_scripts": [
        {
            "matches": ["http://www.nytimes.com/*"],
            "js": ["jquery-1.5.1.js", "prefetch_nytimes.js"],
            "run_at": "document_end"
        }
    ]
}
